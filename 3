/**
 * #define _AYSTL_DECL_COMPARE(...)
 * #include "aystl/global/compare_def.inl"
 * #undef _AYSTL_DECL_COMPARE
 */

_AYSTL_DECL_COMPARE(EQ, ==)
_AYSTL_DECL_COMPARE(NE, !=)
_AYSTL_DECL_COMPARE(LT, <)
_AYSTL_DECL_COMPARE(LE, <=)
_AYSTL_DECL_COMPARE(GT, >)
_AYSTL_DECL_COMPARE(GE, >=)

